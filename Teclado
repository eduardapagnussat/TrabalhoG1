package TrabalhoG1;

public class Teclado {
    
    public String marca;
    public String padrao;
    public float valorMedio;
    public float peso; 
    

    // Construtores 
    public Teclado(){}

    public Teclado(String marca, String padrao;, float valorMedio, float peso){

        this.marca = marca;
        this.padrao = padrao
        this.valorMedio = valorMedio;
        this.peso = peso;

    }
}
